{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"noImplicitAny": false,
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"noEmitOnError": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitThis": true,
		"skipLibCheck": true,
		"incremental": true
	}
}
